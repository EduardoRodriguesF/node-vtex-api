{
  "name": "@vtex/api",
  "version": "0.11.5",
  "description": "VTEX I/O API client",
  "main": "./lib/index.js",
  "scripts": {
    "clean": "rimraf lib/",
    "ava": "ava",
    "test": "run-s lint flow ava",
    "test-watch": "ava --watch",
    "watch": "babel src --out-dir lib --watch --ignore=*.test.js",
    "build": "babel src --out-dir lib --ignore=*.test.js",
    "flow": "flow check",
    "copy": "flow-copy-source -v src lib --ignore=*.test.js",
    "lint": "eslint src",
    "prepublish": "run-s clean build copy test"
  },
  "engines": {
    "node": ">=4"
  },
  "keywords": [
    "vtex"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:vtex/node-vtex-api.git"
  },
  "license": "MIT",
  "dependencies": {
    "axios": "^0.14.0",
    "mime-types": "^2.1.12",
    "multipart-stream": "^2.0.1"
  },
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-cli": "^6.16.0",
    "babel-eslint": "^6.1.2",
    "babel-plugin-transform-flow-strip-types": "^6.14.0",
    "babel-preset-es2015-node4": "^2.1.0",
    "babel-register": "^6.16.3",
    "eslint": "^2.10.2",
    "eslint-config-vtex": "^4.0.0",
    "eslint-plugin-flowtype": "^2.19.0",
    "eslint-plugin-import": "^2.0.1",
    "fast-async": "^6.0.34",
    "flow-bin": "^0.33.0",
    "flow-copy-source": "^1.1.0",
    "npm-run-all": "^2.3.0",
    "rimraf": "^2.5.2"
  },
  "ava": {
    "failFast": true,
    "require": [
      "babel-register"
    ]
  }
}
